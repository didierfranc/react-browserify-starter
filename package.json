{
  "name": "react-browserify-starter",
  "description": "Get started with React using Browserify, Watchify and Express",
  "main": "index.js",
  "dependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "body-parser": "^1.15.0",
    "browserify": "^11.0.1",
    "express": "^4.13.4",
    "jquery": "^2.2.3",
    "marked": "^0.3.5",
    "react": "15.0.1",
    "react-dom": "15.0.1",
    "watchify": "^3.4.0"
  },
  "scripts": {
    "build": "browserify ./public/index.js -t babelify -o ./public/bundle.js",
    "start": "node server.js & watchify ./public/index.js -v -t babelify -o ./public/bundle.js"
  }
}
